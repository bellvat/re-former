
<div>
  <%= form_for @user, url: {action: "update"} do |f| %>
    <%= render 'shared/error_messages' %>
    <div>
      <%= f.label :username%>
      <%= f.text_field :username %>
    </div>
    <div>
      <%= f.label :email%>
      <%= f.text_field :email %>
    </div>
    <div>
      <%= f.label :password%>
      <%= f.text_field :password %>
    </div>
    <div><%= f.submit "Submit" %> </div>
  <% end %>
</div>
